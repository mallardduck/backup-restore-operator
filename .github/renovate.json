{
  "extends": [
    "github>rancher/renovate-config#release",
    "group:kubernetes"
  ],
  "baseBranches": [
    "gha-testtesttest"
  ],
  "ignoreDeps": [
    "github.com/rancher/hull",
    "github.com/rancher/lasso",
    "github.com/rancher/wrangler",
    "github.com/rancher/wrangler/v2",
    "github.com/rancher/wrangler/v3"
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "GitHub Actions",
      "groupSlug": "gha-deps",
      "matchManagers": [
        "github-actions"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    },
    {
      "groupName": "rancher packages",
      "groupSlug": "rancher-go",
      "matchDatasources": [
        "go"
      ],
      "matchPackagePrefixes": [
        "github.com/rancher"
      ]
    }
  ]
}
